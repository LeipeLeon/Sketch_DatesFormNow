{
  "name": "DatesFromNow",
  "version": "0.1.0",
  "description": "Simple data plugin to fill dates from now",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "DatesFromNow",
    "manifest": "src/manifest.json",
    "main": "DatesFromNow.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "Leon Berenschot <leon@wendbaar.nl>",
  "repository": {
    "type": "git",
    "url": "git@github.com:LeipeLeon/Sketch_DatesFormNow.git"
  }
}
